<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %} - G.V.D</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('bootstrap') }}
            {{ encore_entry_link_tags('fontawesome') }}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {{ encore_entry_script_tags('jquery') }}
            {{ encore_entry_script_tags('functions') }}
        {% endblock %}
    </head>
    <body class="bg-form">
        <div class="div-container-form flex-1 my-5">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
